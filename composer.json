{
    "name": "escarter/activity-log",
    "description": "A Laravel package for logging user activity with admin and user views",
    "version": "1.0.0",
    "keywords": [
        "laravel",
        "activity",
        "log",
        "audit",
        "user activity"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "escarter",
            "email": "mbutuhescarter@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/database": "^8.0|^9.0|^10.0",
        "illuminate/queue": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Escarter\\ActivityLog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Escarter\\ActivityLog\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Escarter\\ActivityLog\\ActivityLogServiceProvider"
            ],
            "aliases": {
                "ActivityLog": "Escarter\\ActivityLog\\ActivityLogFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}